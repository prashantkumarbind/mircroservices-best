package com.pksoftware.entity;

import jakarta.persistence.Entity;

@Entity
public class Cunsumer {

		private String name;
		private String address;
		private String accountNo;
		private String gender;
		private float  amount;
		private String mobileNo;
		private int age;
		private String purchegeDate;
		private String purchegeAmount;
		
		
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		public String getAddress() {
			return address;
		}
		public void setAddress(String address) {
			this.address = address;
		}
		public String getAccountNo() {
			return accountNo;
		}
		public void setAccountNo(String accountNo) {
			this.accountNo = accountNo;
		}
		public String getGender() {
			return gender;
		}
		public void setGender(String gender) {
			this.gender = gender;
		}
		public float getAmount() {
			return amount;
		}
		public void setAmount(float amount) {
			this.amount = amount;
		}
		public String getMobileNo() {
			return mobileNo;
		}
		public void setMobileNo(String mobileNo) {
			this.mobileNo = mobileNo;
		}
		public int getAge() {
			return age;
		}
		public void setAge(int age) {
			this.age = age;
		}
		public String getPurchegeDate() {
			return purchegeDate;
		}
		public void setPurchegeDate(String purchegeDate) {
			this.purchegeDate = purchegeDate;
		}
		public String getPurchegeAmount() {
			return purchegeAmount;
		}
		public void setPurchegeAmount(String purchegeAmount) {
			this.purchegeAmount = purchegeAmount;
		}
		
		
}
